{
    "imports": [
        "workspace/scripts/FPSCounterScript.js",
        "workspace/scripts/cameraRotateScript.js",
        "workspace/scripts/cubeRotateScript.js"
    ],

    "updater": [
        {
            "type": "component",
            "class": "ComponentController",
            "children": [
                {
                    "type": "component",
                    "class": "UiController",
                    "attributes": [
                        [
                            {
                                "comment": "Main CSS Path",
                                "value": "./src/workspace/ui/css/main.css",
                                "type": "file"
                            },
                            {
                                "comment": "Ui Scale",
                                "value": "2",
                                "type": "number"
                            }
                        ]
                    ],
                    "children": [
                        {
                            "type": "module",
                            "path": "workspace/modules/ui/fpsCounter.json"
                        }
                    ]
                },
                {
                    "type": "module",
                    "path": "workspace/modules/3d/sceneController.json"
                }
            ]
        }
    ]
}